{
	"name": "app.invoicelink.io",
	"description": "The invoicelink core application",
	"author": "Ratul Maharaj",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --open",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"db:push": "pnpm dlx prisma db push",
		"db:deploy": "pnpm dlx prisma migrate deploy",
		"db:seed": "pnpm dlx prisma db seed",
		"postinstall": "pnpm dlx prisma generate"
	},
	"devDependencies": {
		"@playwright/test": "^1.40.1",
		"@skeletonlabs/skeleton": "^2.6.1",
		"@skeletonlabs/tw-plugin": "^0.3.0",
		"@sveltejs/adapter-auto": "^3.0.1",
		"@sveltejs/adapter-vercel": "^4.0.3",
		"@sveltejs/kit": "^2.0.6",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tailwindcss/forms": "^0.5.7",
		"@types/node": "^20.10.5",
		"@typescript-eslint/eslint-plugin": "^6.16.0",
		"@typescript-eslint/parser": "^6.16.0",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"postcss": "^8.4.32",
		"postcss-import": "^15.1.0",
		"postcss-nesting": "^12.0.2",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.5.9",
		"prisma": "^5.7.1",
		"svelte": "^4.2.8",
		"svelte-check": "^3.6.2",
		"svelte-sequential-preprocessor": "^2.0.1",
		"sveltekit-superforms": "^1.13.1",
		"tailwindcss": "^3.4.0",
		"ts-node": "^10.9.2",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^5.0.10",
		"vitest": "^1.1.0",
		"zod": "^3.22.4"
	},
	"type": "module",
	"engines": {
		"node": ">=18.0.0 <=20.9.0",
		"pnpm": ">=8"
	},
	"dependencies": {
		"@floating-ui/dom": "^1.5.3",
		"@lucia-auth/adapter-prisma": "4.0.0-beta.7",
		"@paralleldrive/cuid2": "^2.2.2",
		"@prisma/client": "^5.7.1",
		"@svelte-put/shortcut": "^3.1.0",
		"arctic": "^0.10.0",
		"cloudinary": "^1.41.1",
		"javascript-time-ago": "^2.5.9",
		"jwt-decode": "^4.0.0",
		"lucia": "3.0.0-beta.12",
		"oslo": "^0.24.0",
		"puppeteer": "^21.6.1",
		"svelte-easy-crop": "^2.0.2",
		"svelte-french-toast": "^1.2.0",
		"tailwind-merge": "^2.2.0"
	}
}