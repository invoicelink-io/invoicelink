{
  "name": "@invoicelink/db",
  "version": "1.2.0",
  "description": "Invoicelink prisma database library",
  "scripts": {
    "build": "pnpm dlx prisma generate",
    "db:push": "pnpm dlx prisma db push --skip-generate",
    "db:migrate": "pnpm dlx prisma migrate dev",
    "db:deploy": "pnpm dlx prisma migrate deploy",
    "db:seed": "pnpm dlx prisma db seed"
  },
  "files": [
    "prisma"
  ],
  "publishConfig": {
    "directory": "prisma"
  },
  "keywords": [],
